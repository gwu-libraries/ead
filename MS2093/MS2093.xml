            <extent altrender="materialtype spaceoccupied">.5 Linear Feet</extent>
            <extent altrender="carrier">1 document box</extent>
          </physdesc>
          <unitdate datechar="creation" normal="1971/1973" type="bulk">1971-1973</unitdate>
