      <c id="aspace_ref58_mua" level="series">
        <did>
          <unittitle>Dance Coalition</unittitle>
          <unitid>MS2143 Series 2</unitid>
          <unitdate datechar="creation">1980-1994</unitdate>
          <physdesc id="aspace_19877f2ed8f40f757a70e0749580dfa7" label="General Physical Description note">0.25 Linear feet</physdesc>
